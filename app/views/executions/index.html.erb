<p id="notice"><%= notice %></p>

<h1>Executions</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Created At</th>
      <th>Updated At</th>
    </tr>
  </thead>

  <tbody>
    <% @executions.each do |execution| %>
      <tr>
        <td><%= execution.status %></td>
        <td><%= execution.created_at %></td>
        <td><%= execution.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
